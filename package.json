{
    "name": "strapi_tools",
    "version": "0.1.0",
    "author": "Justin Konratt",
    "main": "./lib/index.js",
    "bin": {
        "strapi": "./lib/index.js"
    },
    "scripts": {
        "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
        "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" src/index.ts",
        "build": "tsc -p .",
        "global": "npm i -g ."
    },
    "dependencies": {
        "chalk": "^4.1.2",
        "commander": "^9.4.1",
        "figlet": "^1.5.2",
        "path": "^0.12.7"
    },
    "devDependencies": {
        "@types/figlet": "^1.5.5",
        "@types/node": "^18.11.9",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    }
}
